<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#818181"
    tools:context="atividade1.uninove.com.atividade1.MainActivity">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/editText3"
        android:layout_marginRight="8dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/editText4"
        android:layout_marginRight="8dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/editText5"
        android:layout_marginRight="8dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="341dp"
        android:layout_height="98dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/logo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="274dp"
        android:layout_height="38dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="17dp"
        android:text="@string/digite_5_n_meros_inteiros"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:layout_editor_absoluteY="120dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/editText2"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
